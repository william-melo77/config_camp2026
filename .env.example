MYSQL_HOST='localhost'
MYSQL_DATABASE=''
MYSQL_USER=''
MYSQL_PASSWORD=''
PORT=4000
OPENAI_API_KEY=sk...
R2_BUCKET=agentik
R2_ENDPOINT=https://id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=bc...
R2_SECRET_ACCESS_KEY=9...
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
# Cloudflare R2 (S3 API)
# Habilita R2 si vas a usar subida de archivos
R2_ENABLED=false

# Nombre del bucket
R2_BUCKET=agentik

# ¿El bucket permite lectura pública? (si no, se omitirá publicUrl)
R2_BUCKET_PUBLIC=false

# Endpoint S3 de R2 (ej: https://<account-id>.r2.cloudflarestorage.com)
R2_ENDPOINT=

# Credenciales de acceso (Access Key / Secret)
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=

# URL pública base para construir enlaces accesibles
# Ej: https://mi-dominio.com o https://<account-id>.r2.cloudflarestorage.com
R2_PUBLIC_BASE_URL=
